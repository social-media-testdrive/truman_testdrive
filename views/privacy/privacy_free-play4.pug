block content
  -var cdn = "https://dhpd030vnpk29.cloudfront.net";
  .computer.only.tablet.only.four.wide.column 
    .ui.sticky.sideMenu
      .ui.card
        .image
          img(src=cdn+"/post_pictures/lilyprofpic.png")
        .content
          a.header= 'Lily Hui'
          .meta="@"+'hui_lily'
