          span='Sarah Stella'
      .image
        img.post(src=cdn+'/post_pictures/kidbday_se.jpg')
      .content
        .description=`celebrating my younger bro's birthday! <3`
      .three.ui.bottom.attached.icon.buttons
        .ui.reply.button(tabindex='0')
          i.reply.icon
          |  Reply
        .ui.flag.button(tabindex='0')
          i.flag.icon
          |  Flag
        .ui.labeled.button(tabindex='0')
          .ui.like.button
            i.heart.icon
            |  Like
          a.ui.basic.red.left.pointing.label.count=2  
      .content
        .ui.comments
